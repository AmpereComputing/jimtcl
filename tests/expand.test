source testing.tcl

section "Expand Testing"

test expand-1.1 "Basic tests" {
	set a {1 2 3}
	set b {4 5 6}
	lappend a {*}$b
} {1 2 3 4 5 6}

test expand-1.2 "Basic tests" {
	set a {1 2 3}
	set b {4 5 6}
	lappend a {expand}$b
} {1 2 3 4 5 6}

test expand-1.3 "Basic tests" {
	set a {1 2 3}
	set b {4 5 6}
	lappend a *$b
} {1 2 3 {*4 5 6}}

test expand-1.4 "Basic tests" {
	set a {1 2 3}
	set b {4 5 6}
	lappend a expand$b
} {1 2 3 {expand4 5 6}}

testreport
